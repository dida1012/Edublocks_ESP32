{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": false,
        "strictNullChecks": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "jsx": "react",
        "rootDir": "./src",
        "outDir": "./src"
    },
    "include": [
        "src"
    ],
    "exclude": [
        
    ],
    "compileOnSave": true
}